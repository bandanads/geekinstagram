<div class="in">
<h2 class="title">GeekInstagramにログイン</h2>
  <div class="container">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.label :メールアドレス, class: "mp" %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"pm" %>
      </div>

      <div class="field">
        <%= f.label :パスワード, class: "mp" %><br />
        <%= f.password_field :password, autocomplete: "current-password",class:"pm" %>
      </div>
      
      
      <div class="actions">
        <%= f.submit "ログイン",class: "login" %>
      </div>
    <% end %>
    <%= render "users/shared/links" %>

  </div>
</div>